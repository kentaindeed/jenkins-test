pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                touch /var/tmp/test.txt
            }
        }
        stage('build') {
            steps {
                echo 'building'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploy'
            }
        }
        stage('test') {
            steps {
                echo 'test'
            }
        }
        stage('release') {
            steps {
                echo 'release'
            }
        }
    }
}
